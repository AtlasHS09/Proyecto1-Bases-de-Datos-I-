@model IEnumerable<WebMatriculaApp.Entities.Venta>

@{
    ViewData["Title"] = "Ventas";
}

<h1>Ventas por Periodo</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.periodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cobros)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.periodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cobros)
            </td>
        </tr>
}
    </tbody>
</table>
