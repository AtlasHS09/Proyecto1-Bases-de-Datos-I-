@model WebMatriculaNew.Entities.Matricula

@{ ViewData["Title"] = "Delete"; }

<h1>Borrar</h1>

<h3>Esta seguro de Borrarlo?</h3>
<div>
    <h4>Matricula</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CedulaEstudiante)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CedulaEstudiante)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PeriodoMatricular)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PeriodoMatricular)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Ano)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Ano)
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CobrosPendientes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CobrosPendientes)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.MontoMatricula)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MontoMatricula)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CedulaEstudiante" />
        <input type="hidden" asp-for="PeriodoMatricular" />
        <input type="hidden" asp-for="Ano" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar a la Lista</a>
    </form>
</div>
